/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    not(in=sel, out=nsel);
    and(a=a,b=nots,out=w1);
    and(a=b,b=sel,out=w2);
    or(a=w1,b=w2,out=out);
}