/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    and(a=a,b=w4,out=w1);
    and(a=w3,b=b,out=w2);
    not(a=w3);
    not(b=w4);
    or(a=w1,b=w2,out=out);
}